{
  "compilerOptions": {
      "module": "esNext",
      "target": "es5",
      "strict": true,
      "moduleResolution": "node",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": false,
      "allowJs": true,
      "lib": ["es2015" , "dom"],
      "baseUrl": ".",
      "paths": {
        "*": [
          "*",
          "../../../config/vue2/node_modules/*"
        ]
      }
  },
  "include": [
    "src/**/*"
  ]
}
