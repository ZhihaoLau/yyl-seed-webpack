{
  "name": "yyl-seed-webpack",
  "version": "0.3.0-beta13",
  "description": "yyl seed with webpack",
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">= 8.0.0"
  },
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "jest ./test/jest/ --env=node --verbose",
    "ui-test": "yyt",
    "init:base": "node ./test/run.js init --name base --path ./test/__frag",
    "init:vue2": "node ./test/run.js init --name vue2 --path ./test/__frag",
    "init:typescript": "node ./test/run.js init --name typescript --path ./test/__frag",
    "init:vue2-ts": "node ./test/run.js init --name vue2-ts --path ./test/__frag",
    "init:react-ts": "node ./test/run.js init --name react-ts --path ./test/__frag",
    "e-base:all": "node ./test/run.js all --config ./examples/base/config.js",
    "e-base:watch": "node ./test/run.js watch --config ./examples/base/config.js",
    "e-vue2:all": "node ./test/run.js all --config ./examples/vue2/config.js",
    "e-vue2:watch": "node ./test/run.js watch --config ./examples/vue2/config.js",
    "e-typescript:all": "node ./test/run.js all --config ./examples/typescript/config.js",
    "e-typescript:watch": "node ./test/run.js watch --config ./examples/typescript/config.js",
