{
  "name": "yyl-seed-webpack",
  "version": "2.0.0",
  "description": "yyl seed with webpack",
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">= 8.0.0"
  },
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "rules": {
      "node/no-unpublished-require": [
        0
      ],
      "node/exports-style": [
        "error",
        "module.exports"
      ],
      "node/file-extension-in-import": [
        "error",
        "always"
      ],
      "node/prefer-global/buffer": [
        "error",
        "always"
      ],
      "node/prefer-global/console": [
        "error",
        "always"
      ],
      "node/prefer-global/process": [
        "error",
        "always"
      ],
      "node/prefer-global/url-search-params": [
        "error",
        "always"
      ],
      "node/prefer-global/url": [
        "error",
        "always"
      ],
      "node/prefer-promises/dns": "error",
      "node/prefer-promises/fs": "error",
      "quote-props": [
        "error",
        "consistent"
      ]
    }
  },
  "eslintIgnore": [
    "node_modules",
    "/test/case",
    "ui-test",
    "__frag"
  ],
  "scripts": {
    "eslint": "eslint ./",
    "test": "jest ./test/jest/ --env=node --verbose",
    "test:all.case.demo": "jest ./test/jest/all.case.demo.test.js --env=node --verbose",
    "test:all.case.env-test": "jest ./test/jest/all.case.env-test.test.js --env=node --verbose",
    "test:all.case.local-webpack-config-function": "jest ./test/jest/all.case.local-webpack-config-function.test.js --env=node --verbose",
    "test:all.case.react-github-package": "jest ./test/jest/all.case.react-github-package.test.js --env=node --verbose",
    "test:all.case.tofu-ui": "jest ./test/jest/all.case.tofu-ui.test.js --env=node --verbose",
    "test:all.case.vue-js-url": "jest ./test/jest/all.case.vue-js-url.test.js --env=node --verbose",
    "test:all.case.webpack": "jest ./test/jest/all.case.webpack.test.js --env=node --verbose",
    "test:example": "jest ./test/jest/example.test.js --env=node --verbose",
    "test:init.base": "jest ./test/jest/init.base.test.js --env=node --verbose",
    "test:init.react-ts": "jest ./test/jest/init.react-ts.test.js --env=node --verbose",
    "test:init.vue2-ts": "jest ./test/jest/init.vue2-ts.test.js --env=node --verbose",
    "test:init.vue2": "jest ./test/jest/init.vue2.test.js --env=node --verbose",
    "ui-test": "yyt",
    "init:base": "node ./test/run.js init --name base --path ../__frag",
    "init:vue2": "node ./test/run.js init --name vue2 --path ../__frag",
    "init:vue2-ts": "node ./test/run.js init --name vue2-ts --path ../__frag",
    "init:react-ts": "node ./test/run.js init --name react-ts --path ../__frag",
    "frag:all": "node ./test/run.js all --config ../__frag/config.js",
    "frag:watch": "node ./test/run.js watch --config ../__frag/config.js",
    "frag:watch-isCommit": "node ./test/run.js watch --config ../__frag/config.js --isCommit",
    "c-demo:all": "node ./test/run.js all --config ./test/case/demo/config.js",
    "c-demo:watch": "node ./test/run.js watch --config ./test/case/demo/config.js",
    "c-env-test:all": "node ./test/run.js all --config ./test/case/env-test/config.js --proxy --isCommit",
    "c-env-test:watch": "node ./test/run.js watch --config ./test/case/env-test/config.js  --proxy --isCommit",
    "c-webpack:all": "node ./test/run.js all --config ./test/case/webpack/config.js",
    "c-webpack:watch": "node ./test/run.js watch --config ./test/case/webpack/config.js",
    "c-tofu-ui:watch": "node ./test/run.js watch --config ./test/case/tofu-ui/config.js",
    "c-tofu-ui:watch-isCommit": "node ./test/run.js watch --config ./test/case/tofu-ui/config.js --isCommit",
    "c-vue-js-url:watch": "node ./test/run.js watch --config ./test/case/vue-js-url/config.js",
    "c-vue-js-url:watch-isCommit": "node ./test/run.js watch --config ./test/case/vue-js-url/config.js --isCommit",
    "c-react-github-package:watch": "node ./test/run.js watch --config ./test/case/react-github-package/config.js",
    "c-react-github-package:watch-isCommit": "node ./test/run.js watch --config ./test/case/react-github-package/config.js --isCommit",
    "c-local-wconfig-fn:all": "node ./test/run.js all --config ./test/case/local-webpack-config-type-function/config.js",
    "c-local-wconfig-fn:watch": "node ./test/run.js watch --config ./test/case/local-webpack-config-type-function/config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/yyl-seed-webpack.git"
  },
  "keywords": [
    "yyl",
    "seed",
    "webpack"
  ],
  "author": "jackness",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackness1208/yyl-seed-webpack/issues"
  },
  "homepage": "https://github.com/jackness1208/yyl-seed-webpack#readme",
  "dependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "7.5.2",
    "@babel/plugin-proposal-function-sent": "7.5.0",
    "@babel/plugin-proposal-json-strings": "7.0.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "acorn": "6.0.5",
    "autoprefixer": "9.1.5",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.5",
    "babel-polyfill": "6.26.0",
    "css-loader": "2.0.1",
    "es3ify-webpack-plugin": "0.1.0",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.8.0",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "0.7.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "postcss-px2rem": "0.3.0",
    "prettier": "1.12.1",
    "pug": "2.0.3",
    "pug-loader": "2.4.0",
    "pug-plain-loader": "1.0.0",
    "raw-loader": "1.0.0",
    "sass": "1.25.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "svg-inline-loader": "0.8.0",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "1.1.1",
    "watch": "1.0.2",
    "webpack": "4.41.6",
    "webpack-combine-loaders": "2.0.4",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3",
    "webpack-merge": "4.2.1",
    "yyl-file-replacer": "0.4.1",
    "yyl-fs": "0.3.2",
    "yyl-os": "0.10.0",
    "yyl-seed-response": "0.1.1",
    "yyl-util": "2.3.3"
  },
  "devDependencies": {
    "chalk": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-node": "^11.0.0",
    "jest": "^24.9.0",
    "yyl-hander": "^0.3.1",
    "yyl-print": "^0.7.3",
    "yyl-request": "^0.1.0",
    "yyl-seed-test-util": "^0.4.0"
  }
}
