    "e-react-ts-ie8:all": "node ./test/run.js all --config ./examples/react-ts-ie8/config.js",
    "e-react-ts-ie8:watch": "node ./test/run.js watch --config ./examples/react-ts-ie8/config.js",
    "e-react-ts-ie8:watch-isCommit": "node ./test/run.js watch --config ./examples/react-ts-ie8/config.js --isCommit",
    "frag:all": "node ./test/run.js all --config ./test/__frag/config.js",
